{
  "name": "multi-zopfli",
  "module": "index.ts",
  "type": "module",
  "license": "Zlib",
  "author": {
    "name": "narazaka"
  },
  "scripts": {
    "build": "bun run build:bun && bun run build:cp",
    "build:bun": "bun build ./index.ts --compile --outfile multi-zopfli --external zopflipng-bin",
    "build:cp": "mkdir -p dist/node_modules && shx mv multi-zopfli.exe dist && shx cp -R node_modules/zopflipng-bin node_modules/bin-build node_modules/bin-wrapper node_modules/import-lazy node_modules/pify dist/node_modules",
    "clean": "shx rm -rf dist"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/zopflipng-bin": "^4.1.2",
    "shx": "^0.3.4"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@types/file-size": "^1.0.3",
    "cmd-ts": "^0.13.0",
    "fast-glob": "^3.3.2",
    "file-size": "^1.0.0",
    "globby": "^14.0.1",
    "p-queue": "^8.0.1",
    "slash": "^5.1.0",
    "zopflipng-bin": "^7.1.0"
  }
}